		</android.support.v7.preference.PreferenceScreen>
		<Preference
			android:title="divider"
			android:selectable="false"
			android:layout="@layout/divider_preference"/>
	</android.support.v7.preference.PreferenceCategory>
 </android.support.v7.preference.PreferenceScreen>
